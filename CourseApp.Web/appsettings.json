{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      },
      "WriteTo": ["Console"],
      "Enrich": [ "FromLogContext" ]
